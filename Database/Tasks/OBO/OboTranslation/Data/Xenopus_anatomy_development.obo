[Term]
id: XAO:1000001
name: NF stage 1
namespace: xenopus_developmental_stage
def: "Nieuwkoop and Faber stage 1.\nAge: 0h\nExt. Crit.: One cell stage, shortly after fertilization. Pigmentation darker ventrally than dorsally." [XX:<2>, zfin:ZDB-STAGE-010723-4]
related_synonym: "zygote" []
is_a: XAO:1000012 ! embryo

[Term]
id: XAO:1000002
name: early tailbud
namespace: xenopus_developmental_stage
def: "NF stage 22 to the end of stage 28." [Xenbase:<dbxref111>]
is_a: XAO:1000007 ! tailbud

[Term]
id: XAO:1000003
name: blastula
namespace: xenopus_developmental_stage
